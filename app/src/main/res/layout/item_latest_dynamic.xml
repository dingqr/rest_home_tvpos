<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="364px"
    android:layout_height="42px"
    android:gravity="center_vertical"
    android:background="@drawable/bg_view_14_corners">

    <TextView
        android:id="@+id/dynamic_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10px"
        android:layout_marginRight="12px"
        android:text="[2018-09-23]"
        android:textSize="8sp"
        android:textColor="@color/color_e3e6e5"/>

    <TextView
        android:id="@+id/dynamic_what"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="10px"
        android:text="一级跌倒一级跌倒一级跌倒一级跌倒一级跌倒一级跌倒"
        android:textSize="8sp"
        android:textColor="@color/color_e3e6e5"
        android:maxLines="1"
        android:ellipsize="end"/>

</com.zhy.autolayout.AutoLinearLayout>